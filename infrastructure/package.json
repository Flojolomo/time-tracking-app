{
  "name": "time-tracking-infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "dist/bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "build:lambdas": "npm run build:lambda:timeRecords && npm run build:lambda:projects",
    "build:lambda:timeRecords": "cd lambda/timeRecords && npm run build",
    "build:lambda:projects": "cd lambda/projects && npm run build",
    "build:all": "npm run build && npm run build:lambdas",
    "clean": "rm -rf dist",
    "clean:lambdas": "npm run clean:lambda:timeRecords && npm run clean:lambda:projects",
    "clean:lambda:timeRecords": "cd lambda/timeRecords && npm run clean",
    "clean:lambda:projects": "cd lambda/projects && npm run clean",
    "clean:all": "npm run clean && npm run clean:lambdas",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build:all && cdk deploy",
    "destroy": "cdk destroy",
    "synth": "npm run build:all && cdk synth",
    "diff": "npm run build:all && cdk diff"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "20.11.1",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.1100.3",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.233.0",
    "constructs": "^10.0.0"
  }
}